

.search-txt {
    display: inline-block;
    width: 730rpx;
    height: 70rpx;
    background-color: #efefef;
    background-image: url(data:image/png;base64,);
    background-position: 20rpx center;
    background-repeat: no-repeat;
    background-size: 50rpx;
    border-radius: 8rpx;
    font-size: 26rpx;
}

.search-txt-plc {
    color: #999;
}

.search-bar {
    box-sizing: border-box;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    width: 100%;
 
    font-size:13px;
    background-color: #eee;
    text-align: center;
    border-bottom: 1rpx solid #efefef;
   
}

.search-card {
    position: fixed;
    bottom: 120rpx;
    right:60rpx ;
    z-index: 999;
    width: 80rpx;
    height: 80rpx;
    
}


.search {
    
    width: 100%;
    display: inline-block;
    background:#efefef;
    z-index: 1;
}

.search_not_value {}

.search_not_value .search-delete {
    opacity: 0;
    transform: scale(0, 0);
    -webkit-transform: scale(0, 0);
}

.search-body {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    margin: 16rpx;
    top: 0;
}

.search-input {
    flex: 1;
}

.search-box {
    /* height: 59rpx; */
    background-color: #fff;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    border-radius: 6rpx;
    padding: 0 20rpx;
    height: 63rpx;
    border: 1rpx solid #e6e6ea;
}

.search-icon {
    display: flex;
    align-items: center;
}

.search-icon view {
    /* height: 100%; */
    display: inline-block;
    align-items: center;
}

.search-box input {
    padding: 0 10rpx;
    width: 100%;
    font-size: 26rpx;
}

.search-delete {
    transition: all 0.4s ease-in-out;
    -webkit-transition: all 0.4s ease-in-out;
    color: #7e7f86;
}

.search-btn {
    text-align: center;
    padding: 8rpx 10rpx;
    color: #6288d5;
    font-size: 32rpx;
}

.search-btn view {
    display: inline-block;
}


