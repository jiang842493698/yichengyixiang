.container {
  display:block;
  background-color: #F2f2f2;
 
}
.weui_cell{
    position: relative;
    display: flex;
    padding: 30rpx;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    background-color:#fff;
    border-bottom: 1px solid #f6f6f6;
}
.weui_cell_hd{
  display: inline-block;
  width: 25px;
  margin-right: 5px;
}
.weui_cell_hd image{
  width: 100%;
  height: 25px;
  vertical-align: -2px;
}
.vip{
  width:20px;
  height:20px;
  padding:0px 0 0 5px;
}
.weui_cell_bd{
  display: inline-block;
  padding-left:5px;
}
.weui_cell_bd_p{
  font-size: 15px;
  color: #666;
}