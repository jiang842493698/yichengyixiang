}
.weui_cell_bd_p{
  font-size: 15px;
  color: #666;
}
.badge{
  position: absolute;
  top: 18px;
  right: 40px;
  width: 15px;
  height: 15px;
  line-height: 15px;
  background: #ff0000;
  color: #fff;
  border-radius: 50%;
  text-align: center;
  font-size: 8px;
}
.with_arrow{
  position: absolute;
  top: 20px;
  right: 10px;
  width: 15px;
  height: 15px;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

