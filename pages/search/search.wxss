/**index.wxss**/
@import '/style/search.wxss';
.container{
    padding:0;
    margin:0;
}
.ar_picker{
    width:100%;
    font-size:30rpx;
    color:#000;
    background: #efeff4;
    margin:0;
    padding:10rpx 0rpx;
}

.ar_picker .ar_picker_wrap{
    width: 670rpx;
    background: #fff url(../../images/arrow.png) no-repeat right center;
    background-size:25rpx 14rpx;
    background-position:650rpx;
    border-radius:20rpx;
    padding:10rpx 30rpx;
    margin:10rpx auto;
    border:1px solid #ddd;
}
/* 导航条 */
.nav_top{
    display:flex;
    flex-direction: row;
    justify-content: space-around;
    width:100%;
    box-sizing: border-box;
    padding:2rpx 1rpx;
    height: 100rpx;
    background-color: #fff;
     /* background:url(../../images/black_line.png) repeat;  */
    color:#000;
}
.nav_top_items{
    text-align: center;
    font-size:28rpx;
    line-height: 90rpx;
    border-radius:8rpx;
    box-sizing:border-box;
}
.nav_top_items image{
    width:83rpx;
    height:93rpx;
    display: inline-block;
}
.nav_top{
  height:90rpx;


}
.nav_top .active{
    color:#349af0;
}
.active_red{
    color:#349af0;
}
.active_orange{
    color:#349af0;
}
.active_yellow{
    color:#349af0;
}
.active_green{
    color:#349af0;
}
.active_purple{
    color:#349af0;
}


/* 导航对应内容项 */
.nav_section{
    width:100%;
}
.nav_section_items{
    display:flex;
    flex-direction: row;
    justify-content: space-between;
    padding:10rpx;
    border-bottom:2rpx solid #efefef;
    position: relative;
    left:20rpx;

}
.nav_section_items:active{
    background:#ddd;
}
.nav_section_items .section_image{
    width:250rpx;
    height:220rpx;
}
.nav_section_items .section_image image{
    width:100%;
    height:100%;
}
.nav_section_items .section_cont{
    width:380rpx;
    font-size:26rpx;
    color:#a9a9a9;
    padding-left: 15rpx;
}
.nav_section_items .section_cont view{
    overflow:hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
    display: block;
}
.nav_section_items .section_cont .section_cont_intro{
    white-space:normal;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.nav_section_items .section_cont .section_cont_sub{
    font-size: 30rpx;
    line-height:60rpx;
    color:#000;
    margin-bottom:10rpx;
}
.nav_section_items .section_cont .section_cont_price{
    font-size:34rpx;
    color:#ff9913;
}

.section_cc_peo{
    font-size:28rpx;
    color:#ddd;
    margin-left: 50rpx
}
.nav_section_items .section_book{
    height:80rpx;
    font-size:26rpx;
     /* background:url(../../images/location.png) no-repeat left;  */
    /* background-size:17rpx 28rpx; */
