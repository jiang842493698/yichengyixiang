.container{
    background:#fff;
}
.ar_coverpath{
    width:100%;
    margin:0;
    box-sizing:border-box;
    /* padding:40rpx 20rpx; */
}
.ar_coverpath image{
    width:100%;
    height:390rpx;
}
.ar_cont{
    padding:0rpx 60rpx;
    font-size:30rpx;
    background: #fff;
    margin-bottom:40rpx;
    line-height: 45rpx;
    color:#666666;
    width: 680rpx;
}
.ar_cont .ar_cont_h1{
    width:100%;
    font-size: 32rpx;
    font-weight: 600;
    line-height: 64rpx;
    color:#000;
    /* text-align: center; */
}
.ar_final{
  float: left;
}
.ar_remarkbody{
  overflow: hidden;
  border-bottom: 1px solid #CABBC7;
  margin-top: 20rpx;
}
.ar_remark{
  color: #FCAD00;
}
.ar_score{
  font-size: 50rpx;
}
.ar_descb{
  margin-left: 10px;
}
.ar_scale{
  float: left;
}
.ar_comment{
  float: right;
}
.ar_whole{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #CABBC7;
}
.ar_detail{
  float: right;
}
.ar-date {
  margin-top:80rpx;
  background: #CABBC7;
  padding: 10rpx;
}
.ar_cont .ar_cont_h2{
    width:100%;
    font-size: 30rpx;
    font-weight: 600;
    color:#000;
    padding:60rpx 0rpx;
}
.ar_condition{
  width :100%;
  height: 100rpx;
  border-top: 1px solid #CABBC7;
  margin: 10rpx;
}
.ar_condition_one {
  border: 1px solid #CABBC7;
  float: left;
  overflow: hidden;
  height: 40rpx;
  margin: 20rpx 10rpx;
  padding: 5rpx;
}

/* houselist start */
.nav_section{
    width:100%;
}
/* 导航对应内容项 */
.nav_section{
    width:100%;
}
.nav_section_items{
    display:flex;
    flex-direction: row;
    justify-content: space-between;
    padding:10rpx;
    border-bottom:2rpx solid #ddd;
    position: relative;
}
.nav_section_items:active{
    background:#ddd;
}
.nav_section_items .section_image{
    width:250rpx;
    height:220rpx;
}
.nav_section_items .section_image image{
    width:100%;
    height:100%;
}
.nav_section_items .section_cont{
    width:250rpx;
    font-size:26rpx;
    color:#a9a9a9;
    padding-left: 50rpx;
}
.nav_section_items .section_cont view{
    overflow:hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
    display: block;
}
.nav_section_items .section_cont .section_cont_intro{
    white-space:normal;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.nav_section_items .section_cont .section_cont_sub{
    font-size: 30rpx;
    line-height:60rpx;
    color:#000;
    margin-bottom:10rpx;
}
.nav_section_items .section_cont .section_cont_price{
    font-size:32rpx;
    color:#d24a58;
}
.nav_section_items .section_book{
    height:80rpx;
    font-size:26rpx;
    background:url(../../images/location.png) no-repeat left;
    background-size:17rpx 28rpx;
    line-height: 80rpx;
    color:#d24a58;
    text-align: left;
    margin-top:150rpx;
    padding-left:30rpx;
}
/* houselist end */
.ar_sevice_item{
    /* display: flex; */
    flex-direction: row;
    justify-content: space-between;
    /* border-bottom:1px solid #ddd; */
    padding:10rpx 0rpx;
    vertical-align: middle;
    line-height: 80rpx;
    /* width: 150rpx; */
    margin-left: 10rpx;
    margin-top: 30rpx;
}
.ar_sevice_item  .ar_cont_subject{
    width:280rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.ar_sevice_item .ar_cont_price{
    color: #e64340;
    margin-left:60rpx;
}
.ar_sevice_item .section_book{
    width:80rpx;
    height:80rpx;
    font-size:24rpx;
    border-radius:100%;
    background:#d24a58;
    line-height: 80rpx;
    margin-left:25rpx; 
    color:#fff;
    text-align: center;
}
.ar_bookbtn{
  width: 90rpx;
  height: 60rpx;
  background: #FCAD00;
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
}
.section{
   background:#CABBC7;
   margin:20rpx;
   padding:20rpx

}
.search-bottom {
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 100rpx;
    padding: 10rpx;
    text-align: right;
    background-color: #eee;
    box-shadow: 0 10rpx 60rpx rgba(0, 0, 0, .3);
}